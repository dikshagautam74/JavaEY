package p1;

public class A{
	public int a=100;
	private int b=200;
	protected int c=300;
	int d=400;
	void display() {
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
	
	}
	 
}
class B extends A{
	void display() {
		System.out.println(a);
//		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
	
	}
}

class C{
	A a1=new A();
	void display() {
		System.out.println(a1.a);
//		System.out.println(a1.b);
		System.out.println(a1.c);
		System.out.println(a1.d);
	
	}
}

//public - can access anywhere in the package
//private - only for single class
//protected - we can access anywhere in the package + only subclass in other package
//default - can access anywhere in the package
public class P1 {

	public static void main(String[] args) {
		

	}

}
